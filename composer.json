{
  "name": "spryker/product-offer-gui",
  "description": "ProductOfferGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/gui": "^3.33.0",
    "spryker/kernel": "^3.33.0",
    "spryker/locale": "^3.5.0",
    "spryker/product": "^6.20.0",
    "spryker/product-offer": "^0.5.0",
    "spryker/product-offer-gui-extension": "^0.1.0",
    "spryker/propel-orm": "^1.9.0",
    "spryker/store": "^1.4.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  }
}
